    "openai": "^5.22.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@types/bun": "^1.3.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.5.2",
    "typescript": "^5.0.0"
  }
}
